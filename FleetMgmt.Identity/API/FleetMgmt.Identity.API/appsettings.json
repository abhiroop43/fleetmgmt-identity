{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DatabaseConnection": "__DatabaseConnection__"
  },
  "Audience": {
    "Secret": "__Secret__",
    "Iss": "__Iss__",
    "Aud": "__Aud__"
  },
  "SetExpiryTimeInterval": {
    "ExpiryInMinutes": "__ExpiryInMinutes__"
  },
  "SetLongExpiryTimeInterval": {
    "ExpiryInYears": "__ExpiryInYears__"
  },
  "LongDurationRole": {
    "Role": "__Role__",
    "IsEnabled": true,
    "RestrictUsers": false,
    "RestrictUsersCount": 5,
    "IsTokenValid": true
  },
  "AuthenticationMode": {
    "IsValidateOnlyLogin": "__IsValidateOnlyLogin__"
  },
  "SendGridAPIKey": "__SendGridAPIKey__"
}
